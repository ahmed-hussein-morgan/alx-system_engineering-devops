#!/usr/bin/env bash
#printing "bestschool"
for file in *; do
    if [[ -f "$file" ]]; then
        formatted_name=${file//[0-9]*-/}
        
        if [[ "$formatted_name" == *"-"* ]]; then
            name=${formatted_name//-*/}
            echo "$name"
        else
            echo "$formatted_name"
        fi
    fi
done